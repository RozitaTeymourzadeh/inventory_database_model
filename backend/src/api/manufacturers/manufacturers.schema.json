{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "title": "Manufacturer",
    "description": "The manufacturer",
    "required": [
        "name",
        "address_id"
    ],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id", 
            "type": "integer",
            "title": "The database id of the manufacture schema."
        },
        "name": {
            "$id": "#/properties/name", 
            "type": "string",
            "title": "The name schema."
        },
        "description": {
            "$id": "#/properties/description", 
            "type": "string",
            "title": "The description schema."
        },        
        "website_url": {
            "$id": "#/properties/website_url", 
            "type": "string",
            "title": "The website url schema."
        },
        "address_id": {
            "$id": "#/properties/address_id", 
            "type": "number",
            "title": "The address id schema."
        }
    }
}